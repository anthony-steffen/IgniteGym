    "mysql2": "^2.3.3",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
